<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
              https://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="reporte_tramites"
              pageWidth="595"
              pageHeight="842"
              columnWidth="555"
              leftMargin="20"
              rightMargin="20"
              topMargin="20"
              bottomMargin="20"
              uuid="12345-abcd-67890">

    <!-- Título del Reporte -->
    <title>
        <band height="50">
            <staticText>
                <reportElement x="0" y="0" width="555" height="30"/>
                <textElement textAlignment="Center" verticalAlignment="Middle"/>
                <text><![CDATA[Reporte de Trámites]]></text>
            </staticText>
        </band>
    </title>

    <!-- Columnas del Reporte -->
    <columnHeader>
        <band height="20">
            <textField>
                <reportElement x="0" y="0" width="100" height="20"/>
                <textElement textAlignment="Left"/>
                <textFieldExpression><![CDATA["ID Trámite"]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="100" y="0" width="200" height="20"/>
                <textElement textAlignment="Left"/>
                <textFieldExpression><![CDATA["Nombre Trámite"]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="300" y="0" width="255" height="20"/>
                <textElement textAlignment="Left"/>
                <textFieldExpression><![CDATA["Cliente"]]></textFieldExpression>
            </textField>
        </band>
    </columnHeader>

    <!-- Datos del Reporte -->
    <detail>
        <band height="20">
            <textField>
                <reportElement x="0" y="0" width="100" height="20"/>
                <textElement textAlignment="Left"/>
                <textFieldExpression><![CDATA[$F{id}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="100" y="0" width="200" height="20"/>
                <textElement textAlignment="Left"/>
                <textFieldExpression><![CDATA[$F{nombreTramite}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="300" y="0" width="255" height="20"/>
                <textElement textAlignment="Left"/>
                <textFieldExpression><![CDATA[$F{clienteNombre}]]></textFieldExpression>
            </textField>
        </band>
    </detail>

</jasperReport>
